{
  "name": "engioscope",
  "version": "0.1.112",
  "scripts": {
    "watch:tailwind": "tailwind build -i ui/tailwind.css -o ui/tailwind.output.css --watch",
    "start:vite": "vite --open",
    "start:ui": "npm-run-all --parallel watch:tailwind start:vite",
    "start:server": "nodemon --watch 'backend/**' --ext 'ts,json' --exec 'ts-node backend/dev/start-server.ts'",
    "start": "npm-run-all --parallel start:server start:ui",
    "build:server": "tsc --build backend",
    "prebuild:ui": "NODE_ENV=production tailwind build -i ui/tailwind.css -o ui/tailwind.output.css",
    "build:ui": "vite build",
    "clean": "rimraf dist",
    "update-language-colors": "ts-node backend/scraper/update-language-colors.ts",
    "prebuild": "npm-run-all --parallel clean update-language-colors",
    "build": "npm-run-all --parallel build:server build:ui",
    "serve": "vite preview",
    "test:ci": "CI=true npm run test",
    "test": "vitest --dom",
    "scrape": "DEBUG=* node --max-old-space-size=10240 -r ts-node/register ./backend/dev/scrape.ts",
    "scrape:profiler": "DEBUG=* node --max-old-space-size=10240 -r ts-node/register --prof ./backend/dev/scrape.ts",
    "tsc": "tsc --build backend ui",
    "lint": "eslint \"./**/*.ts\" \"./**/*.tsx\" --cache",
    "scratch": "node -r ts-node/register ./backend/scratch.ts",
    "install:husky": "husky install",
    "prepare": "npm-run-all --parallel install:husky update-language-colors",
    "pre-commit": "npm-run-all --parallel test:ci tsc lint"
  },
  "bin": {
    "engioscope": "dist/backend/cli.js"
  },
  "dependencies": {
    "@zkochan/rimraf": "^2.1.1",
    "abort-controller": "^3.0.0",
    "await-lock": "^2.2.2",
    "chalk": "^4.1.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.4",
    "express": "^4.18.1",
    "express-promise-router": "^4.1.0",
    "express-rate-limit": "^6.4.0",
    "glob": "^8.0.1",
    "md5": "^2.3.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.7",
    "node-html-parser": "^5.3.3",
    "pluralize": "^8.0.0",
    "pretty-ms": "^7.0.1",
    "qs": "^6.7.0",
    "rambda": "^7.0.3",
    "split2": "^4.1.0",
    "tar": "^6.1.11",
    "ua-parser-js": "^1.0.2",
    "yaml": "^2.0.1",
    "yargs": "^17.4.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/compression": "^1.7.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.12",
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.5.0",
    "@types/md5": "^2.3.1",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.31",
    "@types/node-fetch": "^2.5.12",
    "@types/pluralize": "^0.0.29",
    "@types/qs": "^6.9.6",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/react-modal": "^3.12.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/split2": "^3.2.1",
    "@types/tar": "^6.1.1",
    "@types/ua-parser-js": "^0.7.36",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "core-js": "^3.22.4",
    "eslint": "^8.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "happy-dom": "^3.1.1",
    "husky": "^7.0.1",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "query-string": "^7.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-intersection-observer": "^9.1.0",
    "react-modal": "^3.15.1",
    "react-router-dom": "^6.3.0",
    "react-select": "^5.3.1",
    "react-tooltip": "^4.2.21",
    "tailwindcss": "^3.0.24",
    "ts-node": "^10.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.8",
    "vitest": "^0.10.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/znsio/engioscope.git"
  },
  "homepage": "https://github.com/znsio/engioscope/blob/main/README.md"
}
