{
  "name": "engioscope",
  "version": "0.1.183",
  "type": "module",
  "scripts": {
    "watch:tailwind": "tailwind -c ui/tailwind.config.cjs build -i ui/tailwind.css -o ui/tailwind.output.css --watch",
    "start:vite": "vite --open",
    "start:ui": "npm-run-all --parallel watch:tailwind start:vite",
    "start:server": "DEBUG=fetch,cron nodemon --watch 'backend/**' --ext 'ts,json' --exec 'ts-node --esm backend/dev/start-server.ts'",
    "start": "npm-run-all --parallel start:server start:ui",
    "build:server": "tsc --build backend",
    "prebuild:ui": "NODE_ENV=production tailwind -c ui/tailwind.config.cjs build -i ui/tailwind.css -o ui/tailwind.output.css",
    "build:ui": "vite build",
    "clean": "rimraf dist",
    "update-language-colors": "ts-node --esm backend/scraper/update-language-colors.ts",
    "prebuild": "npm-run-all --parallel clean update-language-colors",
    "build": "npm-run-all --parallel build:server build:ui",
    "serve": "vite preview",
    "test:ci": "CI=true npm run test",
    "test": "vitest --dom",
    "parallelised-ci-tasks": "npm-run-all --parallel test:ci tsc lint build",
    "scrape": "DEBUG=* node --max-old-space-size=8192 -- node_modules/.bin/ts-node --esm -P backend/tsconfig.json ./backend/dev/scrape.ts",
    "onboard": "DEBUG=* node --max-old-space-size=8192 -- node_modules/.bin/ts-node --esm -P backend/tsconfig.json ./backend/dev/onboard.ts",
    "scrape:profiler": "DEBUG=* node --max-old-space-size=12288 --expose-gc -r ts-node/register --prof ./backend/dev/scrape.ts",
    "tsc": "tsc --build backend ui",
    "lint": "eslint \"./**/*.ts\" \"./**/*.tsx\" --cache",
    "scratch": "DEBUG=* ts-node --esm ./backend/scratch.ts",
    "install:husky": "husky install",
    "prepare": "npm-run-all --parallel install:husky update-language-colors",
    "pre-commit": "npm-run-all --parallel test:ci tsc lint"
  },
  "bin": {
    "engioscope": "dist/backend/cli.js"
  },
  "engines": {
    "node": ">=16.16"
  },
  "dependencies": {
    "@tanstack/react-query-devtools": "4.20.9",
    "@trpc/server": "10.8.1",
    "@zkochan/rimraf": "2.1.2",
    "abort-controller": "3.0.0",
    "await-lock": "2.2.2",
    "chalk": "5.2.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cron": "2.1.0",
    "cron-time-generator": "1.3.0",
    "debug": "4.3.4",
    "expiry-map": "^2.0.0",
    "express": "4.18.2",
    "express-promise-router": "4.1.1",
    "express-rate-limit": "6.7.0",
    "glob": "8.0.3",
    "html-entities": "2.3.3",
    "md5": "2.3.0",
    "mongoose": "6.8.3",
    "morgan": "1.10.0",
    "ms": "2.1.3",
    "node-fetch": "3.3.0",
    "node-html-parser": "6.1.4",
    "p-limit": "4.0.0",
    "p-memoize": "^7.1.1",
    "pluralize": "8.0.0",
    "pretty-ms": "8.0.0",
    "qs": "6.11.0",
    "rambda": "7.3.0",
    "react-hotkeys-hook": "4.3.2",
    "sort-lib": "^1.0.2",
    "split2": "4.1.0",
    "superjson": "1.12.1",
    "tar": "6.1.13",
    "ua-parser-js": "1.0.32",
    "yaml": "2.2.1",
    "yargs": "17.6.2",
    "zod": "3.20.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
