{
  "name": "project-health-tool",
  "version": "0.1.7",
  "scripts": {
    "watch:tailwind": "tailwind -i ui/tailwind.css -o ui/tailwind.output.css --watch",
    "start:vite": "vite --open",
    "start:ui": "npm-run-all --parallel watch:tailwind start:vite",
    "start:server": "nodemon --watch 'backend/**' --ext 'ts,json' --exec 'ts-node backend/dev/start-server.ts'",
    "start": "npm-run-all --parallel start:ui start:server",
    "build:server": "tsc --build backend",
    "prebuild:ui": "NODE_ENV=production tailwind build -i ui/tailwind.css -o ui/tailwind.output.css",
    "build:ui": "vite build",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:server build:ui",
    "serve": "vite preview",
    "test": "jest backend --testPathIgnorePatterns=./dist --coverage",
    "scrape": "DEBUG=* node -r ts-node/register ./backend/dev/scrape.ts",
    "scrape:profiler": "node -r ts-node/register --prof ./backend/dev/scrape.ts",
    "tsc": "tsc --build backend ui",
    "lint": "eslint \"./**/*.ts\" \"./**/*.tsx\" --cache",
    "scratch": "node -r ts-node/register ./backend/scratch.ts",
    "prepare": "husky install"
  },
  "bin": "./dist/backend/cli.js",
  "dependencies": {
    "await-lock": "^2.1.0",
    "azure-devops-node-api": "^11.0.0",
    "chalk": "^4.1.1",
    "chokidar-cli": "^2.1.0",
    "compression": "^1.7.4",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "express-rate-limit": "^5.3.0",
    "lru-cache": "^6.0.0",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.1",
    "pretty-ms": "^7.0.1",
    "qs": "^6.7.0",
    "ramda": "^0.27.1",
    "yargs": "^17.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/compression": "^1.7.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.12",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^26.0.23",
    "@types/lru-cache": "^5.1.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.0.0",
    "@types/node-fetch": "^2.5.10",
    "@types/qs": "^6.9.6",
    "@types/ramda": "^0.27.42",
    "@types/react": "^17.0.12",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/yargs": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "babel-preset-latest": "^6.24.1",
    "core-js": "^3.15.2",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.4",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.3.8"
  }
}
