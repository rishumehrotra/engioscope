{
  "name": "project-health-tool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "watch:tailwind": "chokidar 'ui/**/*.css' 'ui/**/*.scss' --ignore ui/tailwind.output.css -c 'npm run build:tailwind'",
    "start:react": "vite --open",
    "start:ui": "npm-run-all --parallel watch:tailwind start:react",
    "start:server": "nodemon --watch 'server/**' --ext 'ts,json' --exec 'ts-node server/start-dev-server.ts'",
    "start": "npm-run-all --parallel start:ui start:server",
    "build:server": "tsc --build server",
    "postbuild:server": "rimraf dist/ui",
    "prebuild:ui": "tailwind build ui/tailwind.css -o ui/tailwind.output.css",
    "build:ui": "vite build",
    "build": "npm-run-all --parallel build:server build:ui",
    "serve": "vite preview",
    "test:react": "react-scripts test",
    "test": "jest server --testPathIgnorePatterns=./dist --coverage",
    "scrape": "ts-node --project server/tsconfig.json ./server/dev-scrape.ts",
    "tsc": "tsc --build server ui",
    "lint": "eslint \"./**/*.ts\" --cache",
    "scratch": "node -r ts-node/register ./server/scratch.ts",
    "prepare": "husky install"
  },
  "files": [
    "dist"
  ],
  "bin": "./dist/server/cli.js",
  "dependencies": {
    "azure-devops-node-api": "^11.0.0",
    "chalk": "^2.4.2",
    "chokidar-cli": "^2.1.0",
    "core-js": "^2.6.11",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-promise-router": "^4.1.0",
    "lru-cache": "^5.1.1",
    "ms": "^2.1.2",
    "node-fetch": "^2.6.1",
    "qs": "^6.7.0",
    "ramda": "^0.27.1",
    "yargs": "^13.3.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-es2017": "^7.0.0-beta.53",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/lru-cache": "^5.1.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.14.1",
    "@types/node-fetch": "^2.5.10",
    "@types/qs": "^6.9.6",
    "@types/ramda": "^0.27.42",
    "@types/react": "^17.0.12",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/yargs": "^15.0.14",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.2",
    "@vitejs/plugin-react-refresh": "^1.3.4",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.0",
    "nodemon": "^2.0.9",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^1.5.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5",
    "vite": "^2.3.8"
  }
}